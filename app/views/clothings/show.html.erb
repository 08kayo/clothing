  <div class="contents row" >
    <p><%= @nickname %>さんの投稿一覧</p>
    <% @clothings.each do |clothing| %>
      <div class="content_post" style="background-image: url(<%= clothing.image %>);">
        <%= simple_format(clothing.text) %>
      </div>
    <% end %>
    <%= paginate(@clothings) %>
  </div>